apiVersion: grafana.integreatly.org/v1beta1
kind: GrafanaDashboard
metadata:
  name: {{ dsc_name }}-gitlab-ci-pipeline
  namespace: {{ dsc.grafana.namespace }}
spec:
  datasources:
    - inputName: "DS_PROMETHEUS"
      datasourceName: "Prometheus"
  resyncPeriod: 30s
  instanceSelector:
    matchLabels:
      app: {{ grafana_label }}
  plugins:
    - name: grafana-polystat-panel
      version: 2.1.5
  grafanaCom:
    id: 10620
    revision: 4
