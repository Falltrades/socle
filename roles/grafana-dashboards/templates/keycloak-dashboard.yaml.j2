apiVersion: grafana.integreatly.org/v1beta1
kind: GrafanaDashboard
metadata:
  name: {{dsc_name}}-keycloak
  namespace: {{ dsc.grafana.namespace }}
spec:
  datasources:
    - inputName: "PROMETHEUS_DS"
      datasourceName: "Prometheus"
  resyncPeriod: 30s
  instanceSelector:
    matchLabels:
      app: {{ grafana_label }}
  configMapRef:
    name: {{ dsc_name}}-keycloak-dashboard-definition
    key: json
